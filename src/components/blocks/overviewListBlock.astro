---
import OverviewCard from "../general/overviewCard.astro";
import type { Overview } from "../../../interfaces/overview";

const { data } = Astro.props;

const overviewType = data?.overview_type;
---

<div class="my-10">
  <div class="relative xl:container px-2 z-20">
    <div class="grid grid-cols-2 md:grid-cols-4 gap-5">
      {
        data?.overviews.map((overview: Overview) => (
          <OverviewCard {overview} {overviewType} />
        ))
      }
    </div>
  </div>
</div>
