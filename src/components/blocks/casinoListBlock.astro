import CasinoTable from "../casino/casinoTable.astro"
const { data, isCustom = false } = Astro.props

const createCustomCasinos = (casinos) => {
    return casinos.map(item => item.casino.data)
}

const customCasinos = isCustom && createCustomCasinos(data?.casinoLists)

console.log('customCasinos', customCasinos, isCustom)

---

<CasinoTable casinos={isCustom ? data?.casinosList : data.casinos.data} />