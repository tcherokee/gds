@layer utilities {
  .content-auto {
    content-visibility: auto;
    contain-intrinsic-size: 1px 1000px;
  }
}

@layer components {
  .glowy-bkg {
    background-color: rgba(255, 255, 255, 0.3);
    box-shadow: 0px 0px 12px rgba(63, 230, 252, 0.6);
    backdrop-filter: blur(6px);
    border: 1px solid rgba(255, 255, 255, 0.3);
    padding: 12px;
    border-radius: 6px;
  }

  .catpill {
    background: linear-gradient(
        180deg,
        theme("colors.background[900]") 0%,
        theme("colors.blue[700]") 100%
      ),
      linear-gradient(
        180deg,
        theme("colors.grey[100]") 0%,
        theme("colors.blue[100]") 100%
      );
    color: theme("colors.white");
  }

  .blueFilterBtn {
    background: linear-gradient(180deg, #163183 0%, #070c3f 100%),
      linear-gradient(180deg, #3fe6fc 0%, #00bcd4 100%),
      linear-gradient(180deg, #c2c8ff 0%, #365ccf 100%);
    color: theme("colors.white");
  }

  .gradient-box {
    background-clip: padding-box;
    border: solid 1px transparent;

    &:before {
      content: "";
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      z-index: -1;
      margin: -1px;
      border-radius: inherit;
      background: linear-gradient(
        180deg,
        theme("colors.background[900]") 0%,
        theme("colors.blue[700]") 100%
      );
    }
  }

  .curve {
    position: relative;
    background: linear-gradient(
      to bottom,
      theme("colors.background-back-curve") 0%,
      theme("colors.background-back-curve") 40%,
      theme("colors.background-top-gradient") 41%,
      theme("colors.background-top-gradient") 80%,
      theme("colors.background-bottom-gradient") 100%
    );
    background-size: 100%;
    background-repeat: no-repeat;
    z-index: 9;

    &::after {
      position: absolute;
      top: 0px;
      background: url(data:image/svg+xml;base64,);
      content: "";
      width: 100%;
      height: 500px;
      display: block;
    }

    &::before {
      position: absolute;
      top: 50%;
      content: "";
      display: block;
      background: radial-gradient(
        ellipse at 50% 0%,
        theme("colors.background-back-curve") 72%,
        transparent 72%
      );
      height: 20%;
      width: 100%;
      transform: translateY(-50%);
    }
  }
  .curve-2 {
    position: relative;

    background: linear-gradient(
      180deg,
      theme("colors.background-back-curve") 0%,
      theme("colors.background-back-curve") 40%,
      theme("colors.background-top-gradient") 41%,
      theme("colors.background-top-gradient") 80%,
      theme("colors.background-bottom-gradient") 100%
    );

    &::before {
      position: absolute;
      top: 50%;
      content: "";
      display: block;
      background: radial-gradient(
        ellipse at 50% 0%,
        theme("colors.background-back-curve") 72%,
        transparent 72%
      );
      height: 75px;
      width: 100%;
      transform: translateY(-50%);
    }
  }
}

body {
  @apply text-body-text bg-body-bg;
  font-family:
    Roboto,
    Roboto-fallback,
    -apple-system,
    system-ui,
    BlinkMacSystemFont,
    "Segoe UI",
    "Helvetica Neue",
    Arial,
    sans-serif;
}

.section-wrapper {
  @apply w-full xl:container pt-5 px-3;
}

p {
  @apply text-base mb-5;
}

.btn {
  @apply p-1.5 h-auto text-[16px] font-bold text-center;
  min-height: unset;
  line-height: 1.5;
}
.btn-secondary {
  @apply text-secondary-text rounded-[6px];
  background: linear-gradient(
    180deg,
    theme("colors.secondary-tint") 0%,
    theme("colors.secondary") 100%
  );
  border-top: 1px solid rgba(255, 255, 255, 0.3);
}
.btn-misc {
  @apply text-misc-text rounded-[6px];
  background: linear-gradient(
    180deg,
    theme("colors.misc-tint") 0%,
    theme("colors.misc") 100%
  );
  border-top: 1px solid rgba(255, 255, 255, 0.3);
}
.register-btn {
  @apply text-sm text-white font-bold rounded-[6px] border border-[rgba(255,255,255,0.30)] shadow-[0px_2px_10px_0px_rgba(239,108,0,0.28)];
  background: var(
    --gradients-purple,
    linear-gradient(
      180deg,
      theme("colors.purple.300") 0%,
      theme("colors.purple.500") 100%
    )
  );
}

h1,
h2,
h3,
h4,
h5,
h6,
th {
  @apply capitalize text-blue-700;
  font-family: Lato, Lato-fallback, sans-serif;
}

h1 {
  @apply text-xl leading-[26px] md:text-3xl lg:text-4xl text-white;
  text-shadow: 0px 0px 12px rgba(63, 230, 252, 0.6);
  backdrop-filter: blur(6px);
}

h2 {
  @apply text-lg leading-6 md:text-2xl lg:text-3xl;
}

h3 {
  @apply text-base leading-[22px] md:text-xl lg:text-2xl;
}

h4 {
  @apply text-sm leading-[22px] md:text-xl;
}

h5 {
  @apply text-sm md:text-xl md:leading-6;
}

h6 {
  @apply text-xs md:text-sm md:leading-[18px];
}

.page-wrapper {
  & > div:nth-child(3) {
    &:after {
      position: fixed;
      content: "";
      display: block;
      top: 0;
      left: 0;
      right: 0;
    }
  }
}

