.featured-no-curve {
  background:
    url(data:image/svg+xml;base64,),
    linear-gradient(
      180deg,
      theme("colors.background.700") 0%,
      theme("colors.background.500") 100%
    ),
    #fff repeat;
  background-size: 200px;
}

figure.table {
  @apply mb-6 bg-white w-full rounded-lg overflow-hidden border border-inner-content-table-tr-border text-[16px] leading-[26px];
  a {
    @apply text-quicklinks-btn-color;
  }
}

table {
  @apply w-full border-0 shadow-none bg-white;
  th {
    @apply bg-purple-900 text-white text-left relative py-[5px] px-[15px] table-cell;
  }
  tr {
    @apply border-b border-b-inner-content-table-tr-border last:border-b-0;
  }
  td {
    @apply relative py-[10px] px-3 table-cell last:border-l last:border-l-inner-content-table-tr-border only:border-l-0;
  }
}
:global(table) {
  @apply h-[65px] text-center;
}
:global(th) {
  @apply relative py-[5px] px-[15px] table-cell;
}

.glass-wrapper {
  background-color: rgba(255, 255, 255, 0.3);
  box-shadow: 0px 0px 12px rgba(63, 230, 252, 0.6);
  backdrop-filter: blur(6px);
  border: 1px solid rgba(255, 255, 255, 0.3);
  border-radius: 10px;
  padding: 10px;
}

.auth-bg {
  background:
    url(data:image/svg+xml;base64,),
    linear-gradient(
      180deg,
      theme("colors.background.700") 0%,
      theme("colors.background.500") 100%
    ),
    #fff 50% / cover no-repeat;
  @apply -mb-5;
  .auth-form-container {
    border: 1px solid rgba(255, 255, 255, 0.3);
    background: #dce9f3;
    box-shadow: 0px 0px 12px 0px rgba(63, 230, 252, 0.6);
  }
}
.dashboard-glass-wrapper {
  @apply rounded-xl;
  border: 1px solid rgba(255, 255, 255, 0.3);
  background: rgba(255, 255, 255, 0.36);
  box-shadow: 0px 0px 12px 0px rgba(63, 230, 252, 0.6);
  backdrop-filter: blur(6px);
}
.dashboard-bg {
  background: linear-gradient(
    180deg,
    theme("colors.background.700") 0%,
    theme("colors.background.500") 100%
  );
}

.custom-spinner {
  @apply rounded-[50%] border-[2px] border-white w-4 h-4;
  border-right-color: transparent;
  animation: 0.75s linear infinite spinner-border;
}
@keyframes spinner-border {
  to {
    transform: rotate(360deg);
  }
}
